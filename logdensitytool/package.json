{
  "name": "logdensitytool",
  "displayName": "frontend",
  "description": "This extension enhances your coding workflow by automatically measuring and reporting the log density of files within a repository.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.sendGitHubUrl",
        "title": "Send GitHub URL"
      },
      {
        "command": "extension.analyzeLogDensity",
        "title": "Analyze Log Density"
      }
    ],
    "menus": {
      "editor/context":[
        {
          "when": "resourceLangId == java",
          "command": "extension.analyzeLogDensity",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pfeExtension",
          "title": "[DEV] PFE Extension",
          "icon": "media/send-logging.svg"
        }
      ]
    },
    "views": {
      "pfeExtension": [
        {
          "id": "javaFilesView",
          "name": "Density Analysis Preview"
        },
        {
          "id": "openTabsPreview",
          "name": "Open Tabs Density Preview"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "node-fetch": "^3.3.2"
  }
}
